{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": ["error", {
            "endOfLine": "crlf" // Change to "lf" if using unix based OS
        }],
        "no-console" : 0,
        "no-underscore-dangle" : 0,
        "import/no-dynamic-require": 0
    }
}