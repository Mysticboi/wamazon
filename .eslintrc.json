{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": ["error", {
            "endOfLine": "auto"
        }],
        "no-console": 0,
        "no-underscore-dangle": 0,
        "import/no-dynamic-require": 0,
        "class-methods-use-this": 0
    }
}